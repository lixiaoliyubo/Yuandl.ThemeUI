<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="SystemAccentColor">#3379d9</Color>
    <Color x:Key="SystemAccentPrimaryColor">#559ce4</Color>
    <Color x:Key="SystemAccentSecondaryColor">#80b9ee</Color>
    <Color x:Key="SystemAccentTertiaryColor">#add8ff</Color>
    <SolidColorBrush x:Key="SystemAccentColorBrush" Color="{StaticResource SystemAccentColor}" />
    <SolidColorBrush x:Key="SystemAccentColorPrimaryBrush" Color="{StaticResource SystemAccentPrimaryColor}" />
    <SolidColorBrush x:Key="SystemAccentSecondaryColorBrush" Color="{StaticResource SystemAccentSecondaryColor}" />
    <SolidColorBrush x:Key="SystemAccentTertiaryColorBrush" Color="{StaticResource SystemAccentTertiaryColor}" />

    <Color x:Key="TextFillColorPrimary">#FFFFFF</Color>
    <SolidColorBrush x:Key="TextFillColorPrimaryBrush" Color="{StaticResource TextFillColorPrimary}" />
</ResourceDictionary>
